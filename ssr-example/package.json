{
  "name": "example",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "empty": "trash build && trash dist",
    "prep": "gleam build && yarn rename",
    "rename": "node renameFiles.js",
    "dev": "yarn empty && yarn prep && node viteServer",
    "build": "yarn rename && yarn run build:client && yarn run build:server && node --experimental-specifier-resolution=node prerender",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr build/dev/javascript/ssr/entry-server.jsx --outDir dist/server",
    "preview": "cross-env NODE_ENV=production node viteServer"
  },
  "dependencies": {
    "preact": "10.19.3"
  },
  "devDependencies": {
    "@babel/core": "7.23.6",
    "@babel/preset-env": "7.23.6",
    "@babel/preset-react": "7.23.3",
    "@preact/preset-vite": "^2.7.0",
    "preact-render-to-string": "6.3.1",
    "@babel/plugin-transform-react-jsx":"^7.23.4",
    "rollup-plugin-esbuild":"^6.1.0",
    "esbuild": "0.19.9",
    "fs-extra": "11.2.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "4.18.2",
    "glob": "^10.3.10",
    "sirv": "^2.0.3",
    "trash-cli": "5.0.0",
    "vite": "^5.0.7"
  }
}
